{% extends 'base.html.twig' %}
{% block title %}Characters page{% endblock %}
{% block body %}
    {% if character is not empty %}
    <h1>Character Info</h1>
    {{ dump(character) }}
    {{ character.name }}<br />
    <img src="{{ character.image }}" />
    {% else %}
    No character found
    {% endif %}
{% endblock %}